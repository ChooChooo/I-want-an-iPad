<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @project.name %>
</p>

<p>
  <strong>Owner:</strong>
  <%= @project.owner %>
</p>

<p>
  <strong>Description:</strong>
  <%= @project.description %>
</p>

<p>
  <strong>Skills/tools used on this project:</strong>
  	<% unless @skill_list.empty? %>
	  <% @skill_list.each do |skill| %>
	  	<% skill = skill.gsub('"', '') %>
	  	<% skill = skill.squeeze(" ").strip  %>
	  	<ul>
	  		<li><%= link_to skill, skill_detail_path(skill) %></li>
	  	</ul>
	  <% end %>
	<% end %>
</p>

<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
