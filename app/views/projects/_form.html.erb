<%= form_for(@project) do |f| %>
  <% if @project.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@project.errors.count, "error") %> prohibited this project from being saved:</h2>

      <ul>
      <% @project.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div id="project_details">
	  <div class="field">
	    <%= f.label :project_name %><br>
	    <%= f.text_field :name %>
	  </div>
	  <div class="field">
	    <%#= f.label :owner %><br>
	    <%#= f.text_field :owner %>
	  </div>
	  <div class="field">
	    <%= f.label :project_description %><br>
	    <%= f.text_area :description %>
	  </div>
  </div>
  
  <div id="project_skills">
	  <div>
	  	add skills/tools:
	  	<div><%= render partial: "shared/skills_list"%></div>
	  	<div><%= render partial: "shared/skills_adder"%></div>
	  </div>
  </div>
  	
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
